{
  "expo": {
    "name": "Meusic",
    "slug": "Meusic",
    "schema": "Meusic",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.devadiman.Meusic",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.devadiman.Meusic"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "API_CLIENT_ID": "your client id",
      "API_Client_Secret": "your client secret",
      "eas": {
        "projectId": "cfa8df44-f330-483d-9594-90865dc70462"
      }
    },
    "plugins": [
      "expo-font"
    ]
  }
}
